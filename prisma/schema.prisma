generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Contactus {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  fname  String?
  lname  String?
  phone  String?
  message  String?
}

model Newsletter {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
}

model Launching {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
}

model Loan {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  phone String @unique
}